<UserControl x:Class="SuperShell.Output.Viewers.NativeObjectViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SuperShell.Output.Viewers"
             mc:Ignorable="d" Margin="100,5" >
    <Grid>
        <Border Height="28.602" VerticalAlignment="Top" BorderBrush="#FF958E8E" BorderThickness="1" Background="#FFFEFFED">
            <Grid>
                <Expander x:Name="expander" Header="Expander" HorizontalAlignment="Left" Margin="3.942,6.106,0,4.996" Width="17.5" Collapsed="expander_Collapsed" Expanded="expander_Expanded">
                    <Grid Background="#FFE5E5E5"/>
                </Expander>
                <local:OutputWithType x:Name="txtTitle" Margin="21.442,6.106,6.017,4.996" />
            </Grid>
        </Border>
        <StackPanel Margin="10,28.602,0,0" Height="0" d:LayoutOverrides="VerticalAlignment, TopMargin, BottomMargin" x:Name="stackPanel" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto"/>
    </Grid>
</UserControl>
